{
    "id": "fk-read-state",
    "className": "ReadState",
    "name": "Read state",
    "config": {
        "category": "Global state",
        "outputs": 1,
        "inputs": 1,
        "fields": [
            { "id": "state", "label": "State", "type": "text", "required": true, "dataType": "fk-state"},
            { "id": "split_list", "label": "Split list to sequence of messages", "type": "checkbox", "required": true}
        ],
        "onSave": "function () { this.name = $('#node-input-state option:selected').text(); }",
        "label": "function () { return this.name ? 'Read ' + this.name : 'Read state'; }",
        "color": "#FFCC00",
        "icon": "font-awesome/fa-database"
    },
    "help": "Help..."
}